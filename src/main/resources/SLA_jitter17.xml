<rrd_graph_def>
      <!--<filename>test2.png</filename>-->
      <span>
          <start>${start}</start>
          <end>${end}</end>
      </span>
      <options>
          <use_pool>false</use_pool>
          <anti_aliasing>true</anti_aliasing>
          <time_grid>
              <show_grid>true</show_grid>
              <!-- allowed units: second, minute, hour, day, week, month, year -->
              <!--<minor_grid_unit>minute</minor_grid_unit>
              <minor_grid_unit_count>60</minor_grid_unit_count>
              <major_grid_unit>hour</major_grid_unit>
              <major_grid_unit_count>2</major_grid_unit_count>-->
              <!--<label_unit>hour</label_unit>
              <label_unit_count>2</label_unit_count>
              <label_span>1200</label_span>-->
              <!-- use SimpleDateFormat or strftime-like format to format labels -->
              <!--<label_format>dd-MMM-yy</label_format>-->
          </time_grid>
          <!--<value_grid>
              <show_grid>true</show_grid>
              <grid_step>100.0</grid_step>
              <label_factor>5</label_factor>
          </value_grid>-->
          <no_minor_grid>true</no_minor_grid>
          <alt_y_grid>true</alt_y_grid>
          <alt_y_mrtg>true</alt_y_mrtg>
          <alt_autoscale>true</alt_autoscale>
          <alt_autoscale_max>true</alt_autoscale_max>
          <units_exponent>1</units_exponent>
          <units_length>7</units_length>
          <vertical_label>${Vlabel}</vertical_label>
          <!--<width>444</width>
          <height>222</height>-->
          <interlaced>true</interlaced>
          <image_info>filename = %s, width=%d, height=%d</image_info>
          <image_format>png</image_format>
          <image_quality>0.8</image_quality>
          <!--<background_image>luka.png</background_image>-->
          <!--<overlay_image>luka.png</overlay_image>-->
          <!--<unit>kilo</unit>-->
          <!--<lazy>false</lazy>-->
          <min_value>0</min_value>
          <!--<max_value>100</max_value>-->
          <!--<rigid>true</rigid>-->
          <!--<base>100</base>-->
          <!--<logarithmic>true</logarithmic>-->
          <!--<colors>
              <canvas>#FFFFFF</canvas>
              <back>#FFFFFF</back>
              <shadea>#AABBCC</shadea>
              <shadeb>#DDDDDD</shadeb>
              <grid>#FF0000</grid>
              <mgrid>#00FF00</mgrid>
              <font>#FFFFFF</font>
              <frame>#EE00FF</frame>
              <arrow>#FF0000</arrow>
          </colors>-->
          <no_legend>false</no_legend>
          <only_graph>false</only_graph>
          <force_rules_legend>false</force_rules_legend>
          <title>${title}</title>
          <step>300</step>
          <fonts>
              <small_font>
                  <name>Lucida Granda</name>
                  <style>plain</style>
                  <size>11</size>
              </small_font>
              <large_font>
              	  <name>Lucida Granda</name>
		  <style>bold</style>
		  <size>16</size>
              </large_font>
          </fonts>
          <first_day_of_week>SUNDAY</first_day_of_week>
      </options>
      <datasources>
          <def>
              <name>rttmin</name>
              <rrd>${rrd_file}</rrd>
              <source>RTTMin</source>
              <cf>AVERAGE</cf>
          </def>
          <def>
              <name>rttmax</name>
              <rrd>${rrd_file}</rrd>
              <source>RTTMax</source>
              <cf>AVERAGE</cf>
          </def>
          <def>
              <name>minOfPos_SD</name>
              <rrd>${rrd_file}</rrd>
              <source>MinOfPos</source>
              <cf>AVERAGE</cf>
          </def>
          <def>
              <name>maxOfPos_SD</name>
              <rrd>${rrd_file}</rrd>
              <source>MaxOfPos</source>
              <cf>AVERAGE</cf>
          </def>
          <def>
              <name>minOfPos_DS</name>
              <rrd>${rrd_file}</rrd>
              <source>MinOfPosDS</source>
              <cf>AVERAGE</cf>
          </def>
          <def>
              <name>maxOfPos_DS</name>
              <rrd>${rrd_file}</rrd>
              <source>MaxOfPosDS</source>
              <cf>AVERAGE</cf>
          </def>
	  <def>
              <name>PacketLossSD</name>
              <rrd>${rrd_file}</rrd>
              <source>PacketLo</source>
              <cf>AVERAGE</cf>
          </def>
          <def>
              <name>PacketLossDS</name>
              <rrd>${rrd_file}</rrd>
              <source>PktLossDS</source>
              <cf>AVERAGE</cf>
          </def>
	  <def>
              <name>OneWayMinSD</name>
              <rrd>${rrd_file}</rrd>
              <source>OWMinSD</source>
              <cf>AVERAGE</cf>
          </def>
          <def>
              <name>OneWayMaxSD</name>
              <rrd>${rrd_file}</rrd>
              <source>OWMaxSD</source>
              <cf>AVERAGE</cf>
          </def>
	  <def>
              <name>OneWayMinDS</name>
              <rrd>${rrd_file}</rrd>
              <source>OWMinDS</source>
              <cf>AVERAGE</cf>
          </def>
          <def>
              <name>OneWayMaxDS</name>
              <rrd>${rrd_file}</rrd>
              <source>OWMaxDS</source>
              <cf>AVERAGE</cf>
          </def>
      </datasources>
      <graph>
          <line>
              <datasource>rttmin</datasource>
              <color>#00FF00</color>
              <legend>RTT Min</legend>
              <width>2</width>
          </line>
          <line>
              <datasource>rttmax</datasource>
              <color>#FFFF00</color>
              <legend>RTT Max</legend>
              <width>2</width>
          </line>
          <line>
              <datasource>minOfPos_SD</datasource>
              <color>#0000FF</color>
              <legend>MinOfPos_SD</legend>
              <width>2</width>
          </line>
          <line>
              <datasource>maxOfPos_SD</datasource>
              <color>#FFFAFA</color>
              <legend>MaxOfPos_SD Max</legend>
              <width>2</width>
          </line>
          <line>
              <datasource>minOfPos_DS</datasource>
              <color>#8B4513</color>
              <legend>MinOfPos_DS</legend>
              <width>2</width>
          </line>
          <line>
              <datasource>maxOfPos_DS</datasource>
              <color>#CD5C5C</color>
              <legend>MaxOfPos_DS \l</legend>
              <width>2</width>
          </line>
          <line>
              <datasource>PacketLossSD</datasource>
              <color>#B22222</color>
              <legend>PacketLossSD</legend>
              <width>2</width>
          </line>
          <line>
              <datasource>PacketLossDS</datasource>
              <color>#FF1493</color>
              <legend>PacketLossDS</legend>
              <width>2</width>
          </line>
	  <line>
              <datasource>OneWayMinSD</datasource>
              <color>#B03060</color>
              <legend>OneWayMinSD</legend>
              <width>2</width>
          </line>
          <line>
              <datasource>OneWayMaxSD</datasource>
              <color>#0000CD</color>
              <legend>OneWayMaxSD</legend>
              <width>2</width>
          </line>
	  <line>
              <datasource>OneWayMinDS</datasource>
              <color>#9020F0</color>
              <legend>OneWayMinDS</legend>
              <width>2</width>
          </line>
          <line>
              <datasource>OneWayMaxDS</datasource>
              <color>#3CB371</color>
              <legend>OneWayMaxDS \l</legend>
              <width>2</width>
          </line>
          <gprint>
		<datasource>rttmax</datasource>
		<cf>AVERAGE</cf>
		<format>Average RTT : %3.0f msec</format>
	  </gprint>
	  <gprint>
		<datasource>rttmax</datasource>
		<cf>MAX</cf>
		<format>Max RTT: %3.0f msec\l</format>
	  </gprint>
	  <gprint>
		<datasource>OneWayMaxSD</datasource>
		<cf>AVERAGE</cf>
		<format>Average OneWay SD : %3.0f msec</format>
	  </gprint>
	  <gprint>
		<datasource>OneWayMaxSD</datasource>
		<cf>MAX</cf>
		<format>Max OneWay SD: %3.0f msec\l</format>
	  </gprint>
	  <gprint>
		<datasource>OneWayMaxDS</datasource>
		<cf>AVERAGE</cf>
		<format>Average OneWay DS : %3.0f msec</format>
	  </gprint>
	  <gprint>
		<datasource>OneWayMaxDS</datasource>
		<cf>MAX</cf>
		<format>Max OneWay DS: %3.0f msec\l</format>
	  </gprint>
          <!--<hrule>
              <value>1250</value>
              <color>#0000FF</color>
              <legend>This is a horizontal rule</legend>
          </hrule>
          <vrule>
              <time>now-6h</time>
              <color>#0000FF</color>
              <legend>This is a vertical rule</legend>
          </vrule>-->
          <!--<comment>Simple comment</comment>
          <comment>One more comment\c</comment>-->
      </graph>
  </rrd_graph_def>